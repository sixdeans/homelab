[caddy_servers]
caddy-proxy ansible_host=192.168.1.100 ansible_user=debian ansible_ssh_private_key_file=~/.ssh/id_rsa

[caddy_servers:vars]
ansible_python_interpreter=/usr/bin/python3

[dns_servers]
dns-server ansible_host=192.168.1.101 ansible_user=debian ansible_ssh_private_key_file=~/.ssh/id_rsa

[dns_servers:vars]
ansible_python_interpreter=/usr/bin/python3

[auth_servers]
tinyauth ansible_host=192.168.1.102 ansible_user=debian ansible_ssh_private_key_file=~/.ssh/id_rsa

[auth_servers:vars]
ansible_python_interpreter=/usr/bin/python3
